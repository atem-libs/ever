
bool    := True | False


True    not := False
False   not := True

/*   another test comment  */
a || b :=
    a ? True -> True | False -> b bool

//    test comment
a && b :=
    a ? False -> False | True -> b bool

b if Then yay Else nay :=
    b ? True -> yay | False -> nay


