
this true that  := this
this false that := that
this and that   := false (that this)
this or that    := that (true this)
boolish not     := true (false boolish)


// "keywords" -- used in func sigs as demo'd below
else := _
then := _


this if boolish _else that :=
    this boolish 321 that

this unless boolish _then that 321 :=
    that boolish this
