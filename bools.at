
tthis true tthat  := tthis
fthis false fthat := fthat
baThis and baThat := false (baThat baThis)
boThis or boThat  := boThat (true boThis)
boolish not     := true (false boolish)


// "keywords" -- used in func sigs as demo'd below
else := _
then := _


this if boolish _else that :=
    this boolish that

this unless boolish _then that :=
    that boolish this
