
this true that  := this
this false that := that
bThis and bThat := bThat bThis false
bThis or bThat  := true bThis bThat
boolish not     := false boolish true


// "keywords" -- used in func sigs as demo'd below
else := _
then := _


this if what _else that :=
    this what that

this unless what _then that :=
    that what this
