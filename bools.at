
tthis true tthat  := tthis
fthis false fthat := fthat
baThis and baThat := baThat baThis false
boThis or boThat  := true boThis boThat
boolish not     := false boolish true


// "keywords" -- used in func sigs as demo'd below
else := _
then := _


this if boolish _else that :=
    this boolish that

this unless boolish _then that :=
    that boolish this
